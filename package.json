{
  "name": "www.greenwoodjs.dev",
  "version": "0.0.1",
  "type": "module",
  "description": "Documentation website for Greenwood",
  "repository": "https://github.com/ProjectEvergreen/www.greenwoodjs.dev",
  "author": "Owen Buckley <owen@thegreenhouse.io>",
  "license": "MIT",
  "keywords": [
    "Greenwood",
    "Project Evergreen"
  ],
  "engines": {
    "node": "^18.12.1"
  },
  "scripts": {
    "clean": "rm -rf public .greenwood",
    "build": "node --experimental-loader ./node_modules/@greenwood/cli/src/loader.js ./node_modules/.bin/greenwood build",
    "dev": "greenwood develop",
    "serve": "npm run clean && npm run build && greenwood serve"
  },
  "dependencies": {
    "geist": "^1.2.0"
  },
  "devDependencies": {
    "@greenwood/cli": "^0.29.1",
    "rimraf": "^5.0.5"
  }
}
